{
  "name": "crud_apis",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "prod": "node src/server.js",
    "dev": "nodemon src/server.js"
  },
  "author": "debanjan",
  "license": "ISC",
  "description": "crud apis project for learning purpose",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.1"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "consistent-return": "error",
    "no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
